# Supabase設定
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Stripe設定
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Stripe価格ID（実際の価格設定）
# Gold Plan
STRIPE_GOLD_MONTHLY_PRICE_ID=your_gold_monthly_price_id_here
STRIPE_GOLD_YEARLY_PRICE_ID=your_gold_yearly_price_id_here
# Platinum Plan
STRIPE_PLATINUM_MONTHLY_PRICE_ID=your_platinum_monthly_price_id_here
STRIPE_PLATINUM_YEARLY_PRICE_ID=your_platinum_yearly_price_id_here

# OpenAI設定
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic設定
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini設定（スキップ）
# GEMINI_API_KEY=your_gemini_api_key_here

# アプリケーション設定
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# データベース接続設定（Supabase統一）
DATABASE_URL=your_database_url_here
SUPABASE_DATABASE_URL=your_supabase_database_url_here

# JWT設定（オプション）
JWT_SECRET=your_jwt_secret_here
