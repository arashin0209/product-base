# Supabase設定
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_DATABASE_URL=postgresql://postgres:[YOUR_DB_PASSWORD]@[HOST]:[PORT]/postgres

# Stripe設定
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
STRIPE_GOLD_PRICE_ID=price_your_gold_price_id_here
STRIPE_PLATINUM_PRICE_ID=price_your_platinum_price_id_here

# AI サービス設定
# OpenAI（必須 - 初期実装で使用）
OPENAI_API_KEY=sk-proj_your_openai_api_key_here

# Anthropic Claude（将来対応予定）
ANTHROPIC_API_KEY=sk-ant_your_anthropic_api_key_here

# Google Gemini（将来対応予定）  
GEMINI_API_KEY=your_gemini_api_key_here

# アプリケーション設定
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# セキュリティ設定
# JWT_SECRET=your_jwt_secret_here  # Supabase Auth使用のため不要

# データベース接続例（実際の値は.env.localに設定）
# SUPABASE_DATABASE_URL の形式:
# postgresql://postgres:[DB_PASSWORD]@[PROJECT_HOST]:[PORT]/postgres